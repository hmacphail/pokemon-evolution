{
  "name": "pokemon-evolution",
  "version": "0.1.0",
  "description": "Pokemon Evolution",
  "main": "index.js",
  "author": "Heather MacPhail",
  "scripts": {
    "start": "node app.js",
    "migrations": "sequelize db:migrate --migrations-path=server/migrations",
    "seed": "sequelize db:seed:all --seeders-path=server/seeders",
    "client": "ng serve --port 3000",
    "server": "nodemon app.js",
    "watch": "watchify app/js/app.js -o app/js/_bundle.js"
  },
  "private": true,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hmacphail/pokemon-evolution.git"
  },
  "dependencies": {
    "@angular/cli": "^6.0.8",
    "@angular/common": "^6.0.6",
    "@angular/compiler": "^6.0.6",
    "@angular/core": "^6.0.6",
    "@angular/forms": "^6.0.6",
    "@angular/http": "^6.0.6",
    "@angular/platform-browser": "^6.0.6",
    "@angular/platform-browser-dynamic": "^6.0.6",
    "@angular/router": "^6.0.6",
    "@fortawesome/fontawesome-pro": "^5.1.0",
    "@ng-bootstrap/ng-bootstrap": "^2.1.1",
    "@swimlane/ngx-datatable": "^13.0.1",
    "body-parser": "^1.18.3",
    "bootstrap": "^4.1.1",
    "core-js": "^2.5.7",
    "express": "^4.16.3",
    "jquery": "^3.3.1",
    "method-override": "^2.3.10",
    "morgan": "^1.9.0",
    "open-sans-fontface": "^1.4.0",
    "pg": "^7.4.3",
    "pg-hstore": "^2.3.2",
    "popper.js": "^1.14.3",
    "rxjs": "^6.2.1",
    "sequelize": "^4.37.10",
    "sequelize-cli": "^4.0.0",
    "zone.js": "^0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.6.8",
    "@angular/compiler-cli": "^6.0.6",
    "@types/node": "^10.3.4",
    "nodemon": "^1.17.5",
    "typescript": "~2.7.2",
    "watchify": "^3.11.0"
  },
  "license": "ISC",
  "keywords": [
    "pokemon",
    "evolution",
    "guide",
    "level",
    "type",
