{
  "name": "pokemon-evolution",
  "version": "0.1.0",
  "description": "Pokemon Evolution",
  "main": "index.js",
  "author": "Heather MacPhail",
  "engines": {
    "node": "8.11.3"
  },
  "scripts": {
    "start": "node app.js",
    "migrations": "sequelize db:migrate --migrations-path=server/migrations",
    "seed": "sequelize db:seed:all --seeders-path=server/seeders",
    "dev": "nodemon app.js",
    "watch": "watchify app/js/app.js -o app/js/_bundle.js"
  },
  "private": true,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hmacphail/pokemon-evolution.git"
  },
  "dependencies": {
    "body-parser": "^1.18.3",
    "express": "^4.16.3",
    "method-override": "^2.3.10",
    "morgan": "^1.9.0",
    "open-sans-fontface": "^1.4.0",
    "pg": "^7.4.3",
    "pg-hstore": "^2.3.2",
    "sequelize": "^4.37.10",
    "sequelize-cli": "^4.0.0",
    "zone.js": "^0.8.26"
  },
  "devDependencies": {
    "nodemon": "^1.17.5",
    "watchify": "^3.11.0"
  },
  "license": "ISC",
  "keywords": [
    "pokemon",
    "evolution",
    "guide",
    "level",
    "type",
    "effect",
    "ability",
    "move",
    "generation"
  ]
}
